{
  "manifest_version": 2,
  "name": "OctoBadges",
  "short_name": "OctoBadges",
  "version": "0.2",
  "description": "Adds travis, pypi, coveralls, and license badges to Github organisation lists.",
  "icons": {
    "128": "img/github_badge.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "lib/zepto.min.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
